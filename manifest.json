{
  "manifest_version": 2,

  "name": "解除右鍵鎖",
  "description": "點擊後解除右鍵鎖的限制",
  "version": "1.0",
  "commands" : {
    "reload_extension" : {
      "suggested_key" : {
        "default" : "Ctrl+Shift+K",
        "windows" : "Ctrl+Shift+K",
        "mac" : "MacCtrl+Shift+K",
        "chromeos" : "Ctrl+Shift+K",
        "linux" : "Ctrl+Shift+K"
      },
      "description" : "shortcut for reloading extensions"
    }
  },
  "browser_action": {    
    "default_icon": "mouse_default.png",
    "default_title": "解除右鍵鎖 !"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
        "48": "mouse_48.png",
        "128": "mouse_128.png"
  }
}